{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "typeRoots": [
        "node_modules/@types"
    ],
      "lib": ["es2015", "dom"],
      "types":["node"],
      "outDir": "./dist",
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true
    }
